<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#8CC63F;
        primary-200:#628D2A;
        primary-300:#F8FCF4;
        accent-100:#F2C94C;
        accent-200:#D0A00F;
        text-100:#dedede;
        text-200:#9e9e9e;
        bg-100:#3B3F36;
        bg-200:#383C33;
        bg-300:#4D5246;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#8CC63F" />
    <SolidColorBrush x:Key="primary-200" Color="#628D2A" />
    <SolidColorBrush x:Key="primary-300" Color="#F8FCF4" />
    <SolidColorBrush x:Key="accent-100" Color="#F2C94C" />
    <SolidColorBrush x:Key="accent-200" Color="#D0A00F" />
    <SolidColorBrush x:Key="text-100" Color="#dedede" />
    <SolidColorBrush x:Key="text-200" Color="#9e9e9e" />
    <SolidColorBrush x:Key="bg-100" Color="#3B3F36" />
    <SolidColorBrush x:Key="bg-200" Color="#383C33" />
    <SolidColorBrush x:Key="bg-300" Color="#4D5246" />

    <Color x:Key="primary-1000">#8CC63F</Color>
    <Color x:Key="primary-2000">#628D2A</Color>
    <Color x:Key="primary-3000">#F8FCF4</Color>
    <Color x:Key="accent-1000">#F2C94C</Color>
    <Color x:Key="accent-2000">#D0A00F</Color>
    <Color x:Key="text-1000">#dedede</Color>
    <Color x:Key="text-2000">#9e9e9e</Color>
    <Color x:Key="bg-1000">#3B3F36</Color>
    <Color x:Key="bg-2000">#383C33</Color>
    <Color x:Key="bg-3000">#4D5246</Color>
</ResourceDictionary>