<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#FFD700;
        primary-200:#ddb900;
        primary-300:#917800;
        accent-100:#C0C0C0;
        accent-200:#626262;
        text-100:#333333;
        text-200:#5c5c5c;
        bg-100:#F7F7F7;
        bg-200:#ededed;
        bg-300:#c4c4c4;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#FFD700" />
    <SolidColorBrush x:Key="primary-200" Color="#ddb900" />
    <SolidColorBrush x:Key="primary-300" Color="#917800" />
    <SolidColorBrush x:Key="accent-100" Color="#C0C0C0" />
    <SolidColorBrush x:Key="accent-200" Color="#626262" />
    <SolidColorBrush x:Key="text-100" Color="#333333" />
    <SolidColorBrush x:Key="text-200" Color="#5c5c5c" />
    <SolidColorBrush x:Key="bg-100" Color="#F7F7F7" />
    <SolidColorBrush x:Key="bg-200" Color="#ededed" />
    <SolidColorBrush x:Key="bg-300" Color="#c4c4c4" />

    <Color x:Key="primary-1000">#FFD700</Color>
    <Color x:Key="primary-2000">#ddb900</Color>
    <Color x:Key="primary-3000">#917800</Color>
    <Color x:Key="accent-1000">#C0C0C0</Color>
    <Color x:Key="accent-2000">#626262</Color>
    <Color x:Key="text-1000">#333333</Color>
    <Color x:Key="text-2000">#5c5c5c</Color>
    <Color x:Key="bg-1000">#F7F7F7</Color>
    <Color x:Key="bg-2000">#ededed</Color>
    <Color x:Key="bg-3000">#c4c4c4</Color>
</ResourceDictionary>