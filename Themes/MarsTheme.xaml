<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="primary-100" Color="#FF5733" />
    <SolidColorBrush x:Key="primary-200" Color="#ff8a5f" />
    <SolidColorBrush x:Key="primary-300" Color="#fff3bf" />
    <SolidColorBrush x:Key="accent-100" Color="#FFC300" />
    <SolidColorBrush x:Key="accent-200" Color="#916600" />
    <SolidColorBrush x:Key="text-100" Color="#FFFFFF" />
    <SolidColorBrush x:Key="text-200" Color="#c0c0c0" />
    <SolidColorBrush x:Key="bg-100" Color="#1C1C1C" />
    <SolidColorBrush x:Key="bg-200" Color="#2b2b2b" />
    <SolidColorBrush x:Key="bg-300" Color="#434343" />

    <Color x:Key="primary-1000">#FF5733</Color>
    <Color x:Key="primary-2000">#ff8a5f</Color>
    <Color x:Key="primary-3000">#fff3bf</Color>
    <Color x:Key="accent-1000">#FFC300</Color>
    <Color x:Key="accent-2000">#916600</Color>
    <Color x:Key="text-1000">#FFFFFF</Color>
    <Color x:Key="text-2000">#c0c0c0</Color>
    <Color x:Key="bg-1000">#1C1C1C</Color>
    <Color x:Key="bg-2000">#2b2b2b</Color>
    <Color x:Key="bg-3000">#434343</Color>
</ResourceDictionary>