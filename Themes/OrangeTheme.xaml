<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="primary-100" Color="#FF6600" />
    <SolidColorBrush x:Key="primary-200" Color="#ff983f" />
    <SolidColorBrush x:Key="primary-300" Color="#ffffa1" />
    <SolidColorBrush x:Key="accent-100" Color="#F5F5F5" />
    <SolidColorBrush x:Key="accent-200" Color="#929292" />
    <SolidColorBrush x:Key="text-100" Color="#1d1f21" />
    <SolidColorBrush x:Key="text-200" Color="#444648" />
    <SolidColorBrush x:Key="bg-100" Color="#ffffff" />
    <SolidColorBrush x:Key="bg-200" Color="#f5f5f5" />
    <SolidColorBrush x:Key="bg-300" Color="#cccccc" />

    <Color x:Key="primary-1000">#FF6600</Color>
    <Color x:Key="primary-2000">#ff983f</Color>
    <Color x:Key="primary-3000">#ffffa1</Color>
    <Color x:Key="accent-1000">#F5F5F5</Color>
    <Color x:Key="accent-2000">#929292</Color>
    <Color x:Key="text-1000">#1d1f21</Color>
    <Color x:Key="text-2000">#444648</Color>
    <Color x:Key="bg-1000">#ffffff</Color>
    <Color x:Key="bg-2000">#f5f5f5</Color>
    <Color x:Key="bg-3000">#cccccc</Color>

</ResourceDictionary>