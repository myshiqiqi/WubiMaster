<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="primary-100" Color="#FFA500" />
    <SolidColorBrush x:Key="primary-200" Color="#dd8900" />
    <SolidColorBrush x:Key="primary-300" Color="#885400" />
    <SolidColorBrush x:Key="accent-100" Color="#808080" />
    <SolidColorBrush x:Key="accent-200" Color="#2b2b2b" />
    <SolidColorBrush x:Key="text-100" Color="#333333" />
    <SolidColorBrush x:Key="text-200" Color="#5c5c5c" />
    <SolidColorBrush x:Key="bg-100" Color="#EFEFEF" />
    <SolidColorBrush x:Key="bg-200" Color="#e5e5e5" />
    <SolidColorBrush x:Key="bg-300" Color="#bdbdbd" />

    <Color x:Key="primary-1000">#FFA500</Color>
    <Color x:Key="primary-2000">#dd8900</Color>
    <Color x:Key="primary-3000">#885400</Color>
    <Color x:Key="accent-1000">#808080</Color>
    <Color x:Key="accent-2000">#2b2b2b</Color>
    <Color x:Key="text-1000">#333333</Color>
    <Color x:Key="text-2000">#5c5c5c</Color>
    <Color x:Key="bg-1000">#EFEFEF</Color>
    <Color x:Key="bg-2000">#e5e5e5</Color>
    <Color x:Key="bg-3000">#bdbdbd</Color>

</ResourceDictionary>