<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="primary-100" Color="#FF4081" />
    <SolidColorBrush x:Key="primary-200" Color="#ff79b0" />
    <SolidColorBrush x:Key="primary-300" Color="#ffe4ff" />
    <SolidColorBrush x:Key="accent-100" Color="#536DFE" />
    <SolidColorBrush x:Key="accent-200" Color="#ffffff" />
    <SolidColorBrush x:Key="text-100" Color="#333333" />
    <SolidColorBrush x:Key="text-200" Color="#5c5c5c" />
    <SolidColorBrush x:Key="bg-100" Color="#F2F2F2" />
    <SolidColorBrush x:Key="bg-200" Color="#e8e8e8" />
    <SolidColorBrush x:Key="bg-300" Color="#bfbfbf" />

    <Color x:Key="primary-1000">#FF4081</Color>
    <Color x:Key="primary-2000">#ff79b0</Color>
    <Color x:Key="primary-3000">#ffe4ff</Color>
    <Color x:Key="accent-1000">#536DFE</Color>
    <Color x:Key="accent-2000">#ffffff</Color>
    <Color x:Key="text-1000">#333333</Color>
    <Color x:Key="text-2000">#5c5c5c</Color>
    <Color x:Key="bg-1000">#F2F2F2</Color>
    <Color x:Key="bg-2000">#e8e8e8</Color>
    <Color x:Key="bg-3000">#bfbfbf</Color>

</ResourceDictionary>