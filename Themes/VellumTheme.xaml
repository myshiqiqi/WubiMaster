<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="primary-100" Color="#810000" />
    <SolidColorBrush x:Key="primary-200" Color="#630000" />
    <SolidColorBrush x:Key="primary-300" Color="#F0F5F9" />
    <SolidColorBrush x:Key="accent-100" Color="#788189" />
    <SolidColorBrush x:Key="accent-200" Color="#e1e4e6" />
    <SolidColorBrush x:Key="text-100" Color="#1E2022" />
