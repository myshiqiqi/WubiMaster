<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="primary-100" Color="#1F3A5F" />
    <SolidColorBrush x:Key="primary-200" Color="#4d648d" />
    <SolidColorBrush x:Key="primary-300" Color="#acc2ef" />
    <SolidColorBrush x:Key="accent-100" Color="#3D5A80" />
    <SolidColorBrush x:Key="accent-200" Color="#cee8ff" />
    <SolidColorBrush x:Key="text-100" Color="#FFFFFF" />
    <SolidColorBrush x:Key="text-200" Color="#e0e0e0" />
    <SolidColorBrush x:Key="bg-100" Color="#0F1C2E" />
    <SolidColorBrush x:Key="bg-200" Color="#1f2b3e" />
    <SolidColorBrush x:Key="bg-300" Color="#374357" />

    <Color x:Key="primary-1000">#1F3A5F</Color>
    <Color x:Key="primary-2000">#4d648d</Color>
    <Color x:Key="primary-3000">#acc2ef</Color>
    <Color x:Key="accent-1000">#3D5A80</Color>
    <Color x:Key="accent-2000">#cee8ff</Color>
    <Color x:Key="text-1000">#FFFFFF</Color>
    <Color x:Key="text-2000">#e0e0e0</Color>
    <Color x:Key="bg-1000">#0F1C2E</Color>
    <Color x:Key="bg-2000">#1f2b3e</Color>
    <Color x:Key="bg-3000">#374357</Color>

</ResourceDictionary>