<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#C7B299;
        primary-200:#a9957d;
        primary-300:#675640;
        accent-100:#A68B5B;
        accent-200:#483507;
        text-100:#4D4D4D;
        text-200:#797979;
        bg-100:#F5EFE7;
        bg-200:#ebe5dd;
        bg-300:#c2bdb5;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#C7B299" />
    <SolidColorBrush x:Key="primary-200" Color="#a9957d" />
    <SolidColorBrush x:Key="primary-300" Color="#675640" />
    <SolidColorBrush x:Key="accent-100" Color="#A68B5B" />
    <SolidColorBrush x:Key="accent-200" Color="#483507" />
    <SolidColorBrush x:Key="text-100" Color="#4D4D4D" />
    <SolidColorBrush x:Key="text-200" Color="#797979" />
    <SolidColorBrush x:Key="bg-100" Color="#F5EFE7" />
    <SolidColorBrush x:Key="bg-200" Color="#ebe5dd" />
    <SolidColorBrush x:Key="bg-300" Color="#c2bdb5" />

    <Color x:Key="primary-1000">#C7B299</Color>
    <Color x:Key="primary-2000">#a9957d</Color>
    <Color x:Key="primary-3000">#675640</Color>
    <Color x:Key="accent-1000">#A68B5B</Color>
    <Color x:Key="accent-2000">#483507</Color>
    <Color x:Key="text-1000">#4D4D4D</Color>
    <Color x:Key="text-2000">#797979</Color>
    <Color x:Key="bg-1000">#F5EFE7</Color>
    <Color x:Key="bg-2000">#ebe5dd</Color>
    <Color x:Key="bg-3000">#c2bdb5</Color>
</ResourceDictionary>